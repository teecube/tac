<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd"
	xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	name="bw-default-project" partial="false">

	<requiredProperties>
		<requiredProperty key="bwProjectName"/>
		<requiredProperty key="ticBW5MavenVersion">
			<defaultValue>${ticBW5MavenVersion}</defaultValue>
		</requiredProperty>
	</requiredProperties>
	<fileSets>
		<fileSet encoding="UTF-8" filtered="true" packaged="false">
			<directory>src</directory>
		</fileSet>
	</fileSets>
</archetype-descriptor>
